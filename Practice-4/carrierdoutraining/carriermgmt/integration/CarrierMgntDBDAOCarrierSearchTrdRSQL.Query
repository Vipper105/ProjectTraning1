<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierMgntDBDAOCarrierSearchTrdRSQL">
			<desc><![CDATA[a]]></desc>
			<sql><![CDATA[
select count(trd_cd) 
from mdm_trade
where trd_cd = @[trd_cd]
    and delt_flg = 'N'				]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
